<launch>
  <param name="robot_description" textfile="$(find thymio_driver)/models/base.urdf"/>

  <node pkg="asebaros" type="asebaros" name="asebaros" output="screen" args="ser:device=/dev/ttyACM0"/>
  <node name="thymio" pkg="thymio_driver" type="thymio_driver_node.py" output="screen" />
<!-->Thymio has no published joints (the wheels position are not published)<-->
<node pkg="robot_state_publisher" type="robot_state_publisher" name="rob_st_pub" ></node>  


</launch>
